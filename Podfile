use_frameworks!

def pods_aarq

    # Pods for Capacitor
    pod 'CapacitorCordova', '2.1.1'

end

target 'Capacitor' do

    platform :ios, '10.3'
    pods_aarq

end

target 'CapacitorTests' do

    platform :ios, '10.3'
    pods_aarq

end
